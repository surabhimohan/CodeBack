package test;
import java.util.*;
public class Program 
{
	//String name,nickName,city,email;
	//int id,number;
		
	public static void main(String[] args)
	{
		try(Scanner s=new Scanner(System.in);)		
		{
			System.out.print("name: ");
			System.out.print("nickname: ");
			System.out.print("email: ");
			System.out.print("city: ");
			
			if(s.hasNext("[A-Z a-z]*"))
					{
				String name=s.next();
				String nickName=s.nextLine();
				String email=s.nextLine();
				String city=s.nextLine();
					System.out.print("Student's Name: "+name);
					System.out.print("Sudent's NickName: "+nickName);
					System.out.println("Student's email: "+email);
					System.out.println("Student's City: "+city);
					}
			else
				System.out.println("invalid input");
		}
	}

}
