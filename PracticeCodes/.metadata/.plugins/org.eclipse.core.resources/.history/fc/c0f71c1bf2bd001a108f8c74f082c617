package test;
import java.util.*;
public class StringR 
{
	public static void main(String[] args) 
	{
		Scanner s=new Scanner(System.in);
		System.out.println("enter the string:");
		String s1=s.next();
		String s2="";
		for(int i=s1.length()-1;i>=0;i--)
		{
			s2=s2+s1.charAt(i);
			System.out.print(s2);
		}
			s.close();
		
	}

}
