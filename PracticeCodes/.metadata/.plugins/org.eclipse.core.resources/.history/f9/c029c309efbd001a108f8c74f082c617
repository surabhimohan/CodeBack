package test;
import java.util.*;
public class StringR 
{
	public static void main(String[] args) 
	{
		Scanner s=new Scanner(System.in);
		System.out.println("enter the string:");
		String s1=s.next();
		String rev="";
		char ch[]=s1.toCharArray();
		for(int i=ch.length-1;i>=0;i--)
		{
			rev+=ch[i];
		}
		System.out.println(rev);
		s.close();
	}

}
