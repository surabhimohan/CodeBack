package test;
import java.util.*;
public class StringR 
{
	public static void main(String[] args) 
	{
		Scanner s=new Scanner(System.in);
		System.out.println("enter the string:");
		String a=s.nextLine();
		for(int i=a.length()-1;i>=0;i--)
		{
			char[] ch=new char[i];
			System.out.print(ch);
		}
		s.close();
	}

}
