package test;

public class StringP
{
	public static void main(String[] args)
	{
		java.util.Scanner s=new java.util.Scanner(System.in);
		String s1=s.next();
		String s2="";
		for(int i=s1.length()-1;i>=0;i--)
		{
			s2=s2+s1.charAt(i);
		}
		if(s2.equals(s1))
			System.out.println("is p");
		else
			System.out.println("not p");
		s.close();
	}

}
